algoritmo "CREUZA_DOLARES"
// CREUZA VAI VIAJAR E PRECISA TROCAR SEUS REAIS EM DOLARES
var
   RS, Dolar: REAL
   
inicio
      Escreval ("Quantos R$ a Creuza possui?")
      leia (RS)
      Dolar <- RS/5.38
      Escreval ("Posso trocar meus reais no total de", dolar," US$")
fimalgoritmo


// 1US$ = R$5.38