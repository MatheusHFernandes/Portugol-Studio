algoritmo "VETORES"
var
   V: vetor[1..5] de inteiro
   C: inteiro

inicio

      Para C <- 1 ate 5 faca
      Escreva("Digite o ",c,"º valor: ")    //INSERE OS VALORES NO VETOR
      leia (V[C])
      fimpara
      
      para C <- 1 ate 5 faca
      Escreval("[", V[C], "]")       //ESCREVE OS VALORES ARMAZENADOS
      fimpara
      
fimalgoritmo